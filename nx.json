{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "seed": {
      "dependsOn": ["^seed"],
      "cache": true
    },
    "start": {
      "dependsOn": ["^start"]
    },
    "dev": {
      "dependsOn": ["^dev"]
    }
  },
  "defaultBase": "main",
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "eslint:lint"
      }
    }
  ]
}
